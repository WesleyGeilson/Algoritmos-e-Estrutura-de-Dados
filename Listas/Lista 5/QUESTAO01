Multilistas(k,vet['1...n'],totalElementos){
    tam_bloco = âˆštotalElementos // queremos pegar a quantidade total de elemento e encontrar um numero ideial para as sublistas
    para (i = 0 ate totalElementos/tam_blocos - 1) { // A lista perde seus indices 'normais' para a sublista (se a lista tiver 100 pos entao agora ela passa a ter 10 somente)
        inicial_bloco = i*tam_blocos + 1 // crava o indice do bloco inicial
        final_bloco = min(totalElementos, i+1 * tam_bloco) // crava o indice final do bloco 

    }
}
